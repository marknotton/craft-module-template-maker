{% set <FieldHandle> = entry.<FieldHandle>.all() %}

{% if <FieldHandle> is not empty %}

  <h3>{{ "<FieldName>"|t }}</h3>

  <ul class="<FieldClass>">

    {% for user in <FieldHandle> %}

      <li><a href="/{{ user.uri }}">{{ user.firstName|default(user.username) }}</a></li>

    {% endfor %}

  </ul>

{% endif %}
